{
  "name": "IntelligentAuditTools",
  "version": "0.9.0",
  "scripts": {
    "dev:mock": "vite --open --mode mock",
    "dev": "vite --mode development",
    "dev:linux": "vite --mode development",
    "build:test": "vite build --mode test",
    "build": "vue-tsc --noEmit && vite build --mode release",
    "build:site": "vue-tsc --noEmit && vite build --mode site",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less}",
    "prepare": "node -e \"if(require('fs').existsSync('.git')){process.exit(1)}\" || husky install",
    "site:preview": "npm run build && cp -r dist _site",
    "test": "echo \"no test specified,work in process\"",
    "test:coverage": "echo \"no test:coverage specified,work in process\""
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.5",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "echarts": "5.5.0",
    "exceljs": "^4.4.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qrcode.vue": "^3.4.1",
    "qs": "^6.12.1",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.9.4",
    "tvision-color": "^1.6.0",
    "vue": "^3.4.27",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.13.1",
    "vue-router": "~4.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tauri-apps/cli": "^1.5.14",
    "@types/echarts": "^4.9.22",
    "@types/lodash": "^4.17.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.4.27",
    "@vue/eslint-config-typescript": "^13.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-vue-scoped-css": "^2.8.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.7.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.2.5",
    "stylelint": "~16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "~6.0.4",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-mock": "^3.0.2",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.0.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix"
    ],
    "*.{html,vue,css,sass,less}": [
      "npm run stylelint:fix"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "IntelligentAuditTools"
}
