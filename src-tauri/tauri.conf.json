{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build && copy splash\\* dist",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3002",
    "distDir": "../dist"
  },
  "package": {
    "productName": "IntelligentAuditTools",
    "version": "0.2.3"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "cn.cupbread",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix":{
          "language": "zh-CN"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "width": 1280,
        "resizable": true,
        "title": "Intelligent Audit Tools",
        "visible": false,
        "center": true
      },
      {
        "fullscreen": false,
        "height": 200,
        "width": 400,
        "decorations": false,
        "label": "splashscreen",
        "url": "splash.html",
        "center": true,
        "alwaysOnTop": true
      }
    ]
  }
}
